# This file contains configuration for Git.
# Information about gitattributes is avaiable here: [https://git-scm.com/docs/gitattributes]

# text files
*.csv           -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=CSV            merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
 .gitattributes -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=Git-Attributes merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
 .gitignore     -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=Git-Ignore     merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.html          -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=HTML           merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.js            -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=JavaScript     merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.json          -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=JSON           merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.md            -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=Markdown       merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
  mimetype      -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=Text           merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.sh            -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=Shell          merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.svg           -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=SVG            merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.txt           -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=Text           merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.xml           -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=XML            merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8
*.yml           -binary diff encoding=UTF-8 eol=lf git-encoding=UTF-8 linguist-generated=false linguist-language=YAML           merge text working-tree-encoding=UTF-8 zos-working-tree-encoding=UTF-8

# binary files
*.gif            binary -diff -encoding    -eol   -git-encoding       linguist-generated=false -linguist-language              -merge -text -working-tree-encoding    -zos-working-tree-encoding
 .gitkeep        binary -diff -encoding    -eol   -git-encoding       linguist-generated=false -linguist-language              -merge -text -working-tree-encoding    -zos-working-tree-encoding
*.jpg            binary -diff -encoding    -eol   -git-encoding       linguist-generated=false -linguist-language              -merge -text -working-tree-encoding    -zos-working-tree-encoding
*.pdf            binary -diff -encoding    -eol   -git-encoding       linguist-generated=false -linguist-language              -merge -text -working-tree-encoding    -zos-working-tree-encoding
*.png            binary -diff -encoding    -eol   -git-encoding       linguist-generated=false -linguist-language              -merge -text -working-tree-encoding    -zos-working-tree-encoding
*.ttf            binary -diff -encoding    -eol   -git-encoding       linguist-generated=false -linguist-language              -merge -text -working-tree-encoding    -zos-working-tree-encoding
